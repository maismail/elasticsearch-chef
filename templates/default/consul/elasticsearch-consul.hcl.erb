services = [
    {
        id = "elasticsearch/transport"
        name = "elasticsearch"
        tags = [
            "transport",
            "<%= @id %>"
        ]
        port = <%= node['elastic']['ntn_port'] %>
        check = {
            id = "elasticsearch-health-check"
            name = "Health check for Elasticsearch"
            args = ["<%= node['elastic']['bin_dir'] %>/consul/nn-health.sh"]
            interval = "20s"
            timeout = "20s"
        }
    },
    {
        id = "elasticsearch/http"
        name = "elasticsearch"
        tags = [
            "http",
            "<%= @id %>"
        ]
        port = <%= node['elastic']['port'] %>
        check = {
            id = "elasticsearch-http-check"
            alias_service = "elasticsearch/transport"
        }
    }
]
